<h1 class="main-title"> Food Order Owner Dashboard</h1>
<div class="topnav">
    <%= link_to "Home", owners_path %>
    <%= link_to "Menu Details", owner_menus_path %>
     <%= link_to "Order Details", owner_orders_path %>
    <%= button_to "Sign out", owner_sign_out_path , method: :delete%>
</div>
<h1 class="main-header"> Order List </h1>
<table class="menu-table">
  <thead>
    <tr>
      <th>User Id</th>
      <th>Order Id</th>
      <th>Order Date </th>
      <th>Price</th>
      <th>Status <th>
      <th colspan="5"></th>
    </tr>
  </thead>

  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= order.user_id %></td>
        <td><%= order.id%></td>
        <td><%= order.date%></td>
        <td><%= order.price%></td>
        <td><%= order.status%></td>
      </tr>
    <% end %>
  </tbody>
</table>
